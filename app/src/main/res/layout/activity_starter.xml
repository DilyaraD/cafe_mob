                    android:layout_marginLeft="30dp"/>
                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView1377"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:textColor="@color/black"
                    android:textSize="19sp"
                    android:text="Кальмары фритто................500гр      980" />

                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView1477"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="кольца кальмара, обжаренные во фритюре, свежий лимон, соус на выбор, например, маринара или тартар"
                    android:textSize="14sp"
                    android:layout_marginLeft="30dp"/>


                <ImageView
                    android:layout_marginRight="26dp"
                    android:id="@+id/imageView250011"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/image_1411"
                    android:scaleType="fitXY"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="30dp"/>
                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView199"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:textColor="@color/black"
                    android:textSize="19sp"
                    android:text="Каннеллони............................500гр    500" />

                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="трубочки из свежей пасты, заполненные мясным соусом или начинкой, обычно с мясом и сыром, а затем запеченные с томатным соусом и сыром"
                    android:textSize="14sp"
                    android:layout_marginLeft="30dp"/>


                <ImageView
                    android:layout_marginRight="26dp"
                    android:id="@+id/imageView2600"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/image_20"
                    android:scaleType="fitXY"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"/>


                <ImageView
                    android:id="@+id/imageView29"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="perehod_na_menu"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/component_6" />

            </LinearLayout>
        </ScrollView>

    </LinearLayout>


</RelativeLayout>