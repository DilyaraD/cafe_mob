                android:onClick="perehod_na_menu"
                app:srcCompat="@drawable/icons8__50" />
        </RelativeLayout>



        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="9dp">
            <Button
                android:id="@+id/textViewu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="Стартеры"
                android:background="@android:color/transparent"
                android:foreground="?android:attr/selectableItemBackground"
                android:textSize="9sp"
                android:textColor="@color/black"
                />

            <Button
                android:id="@+id/textViewuu"
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@android:color/transparent"
                android:foreground="?android:attr/selectableItemBackground"
                android:onClick="perehod_na_pizza"
                android:text="Пицца"
                android:textColor="@color/black"
                android:textSize="9sp" />

            <Button
                android:id="@+id/textViewuuu"
                android:layout_width="71dp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@android:color/transparent"
                android:foreground="?android:attr/selectableItemBackground"
                android:onClick="perehod_na_dinner"
                android:text="Ужин"
                android:textColor="@color/black"
                android:textSize="9sp" />

            <Button
                android:id="@+id/textViewuuuu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@android:color/transparent"
                android:foreground="?android:attr/selectableItemBackground"
                android:onClick="perehod_na_drinks"
                android:text="Напитки"
                android:textColor="@color/black"
                android:textSize="9sp" />

            <Button
                android:id="@+id/textViewuuuuu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@android:color/transparent"
                android:foreground="?android:attr/selectableItemBackground"
                android:onClick="perehod_na_desert"
                android:text="Десерты"
                android:textColor="@color/black"
                android:textSize="9sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-20dp">
            <TextView
                android:id="@+id/textViewuuuuuu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="- - - - - -"
                android:layout_marginLeft="20dp"
                android:textSize="17sp"
                android:textColor="@color/black"
                />
        </LinearLayout>
        <ScrollView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center_vertical">
                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
