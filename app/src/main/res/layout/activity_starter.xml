                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="30dp"
                    android:textColor="@color/black"
                    android:textSize="19sp"
                    android:text="Капрезе.................................500гр    1080" />
                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView19"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="фета сыр, свежие помидоры, нарезанные кружочками, свежий базилик, оливковое масло, соль и перец"
                    android:textSize="14sp"
                    android:layout_marginLeft="30dp"/>
                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView11011"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="30dp"
                    android:textColor="@color/black"
                    android:textSize="19sp"
                    android:text="Карпаччо...............................500гр    1590" />
                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView1911"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="тунец, соус на выбор, например, соус на основе лимона и оливкового масла или соус бальзамического уксуса"
                    android:textSize="14sp"
                    android:layout_marginLeft="30dp"/>


                <ImageView
                    android:layout_marginRight="26dp"
                    android:id="@+id/imageView230011"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/image_1311"
                    android:scaleType="fitXY"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="30dp"/>
                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:textColor="@color/black"
                    android:textSize="19sp"
                    android:text="Минестроне..........................500гр    2190" />

                <TextView
                    android:layout_marginRight="26dp"
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="свежие овощи: морковь, картофель, цветная капуста, брокколи, бульон, рис, соль, перец и итальянские травы"
